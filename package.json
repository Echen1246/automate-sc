{
  "name": "automate-sc",
  "version": "2.1.0",
  "description": "Snapchat Web automation with AI chatbot and dashboard",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cd dashboard && npm run build",
    "build:backend": "tsc",
    "build:dashboard": "cd dashboard && npm run build",
    "start": "node dist/index.js",
